@model IEnumerable<LainausjarjestelmaMVC.Models.Varastot>

@{
    ViewBag.Title = "Varastopaikat";
}
<br />
<h2>Varastopaikat</h2>

<p>
    @Html.ActionLink("Lisää uusi", "Create")
</p>
<table class="table table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Varastopaikka)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Numero)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Varastopaikka)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numero)
        </td>
        <td>
            @Html.ActionLink("Muokkaa", "Edit", new { id=item.VarastoID }) |
            @Html.ActionLink("Yhteenveto", "Details", new { id=item.VarastoID }) |
            @Html.ActionLink("Poista", "Delete", new { id=item.VarastoID })
        </td>
    </tr>
}

</table>
